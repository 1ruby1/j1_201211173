	}
	
	public void calculateBracket(){
		
		int i=this.bracketList.size()-1;
		String s;
		int [][] bracketSet= new int[1][2]; 
		for(;i>=0;i--){
			new MyCalculator(super.getStr().substring(i+1, ));
			
			s = super.getStr().substring(i-1, i);
			
			if(s.equals("^")){
				
			}else if(s.equals("d")){
				
			}else if(s.equals("p")){
				
			}else if(s.equals("n")){
				
			}else if(s.equals("s")){
				
			}else if(s.equals("g")){
				
			}
				
			
		}
	}
}
